        "@babel/polyfill": "^7.11.5",
        "@babel/preset-env": "^7.11.5",
        "autoprefixer": "^9.8.6",
        "aws-sdk": "^2.761.0",
        "axios": "^0.20.0",
        "babel-cli": "^6.26.0",
        "babel-loader": "^8.1.0",
        "body-parser": "^1.19.0",
        "connect-flash": "^0.1.1",
        "connect-mongo": "^3.2.0",
        "cookie-parser": "^1.4.5",
        "css-loader": "^1.0.1",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "express-session": "^1.17.1",
        "extract-text-webpack-plugin": "^4.0.0-beta.0",
        "helmet": "^4.1.1",
        "mini-css-extract-plugin": "^0.11.2",
        "mongoose": "^5.10.7",
        "morgan": "^1.10.0",
        "multer": "^1.4.2",
        "multer-s3": "^2.9.0",
        "node-sass": "^4.14.1",
        "passport": "^0.4.1",
        "passport-github": "^1.1.0",
        "passport-local": "^1.0.0",
        "passport-local-mongoose": "^6.0.1",
        "postcss-loader": "^3.0.0",
        "prettier": "^2.1.2",
        "pug": "^3.0.0",
        "sass-loader": "^7.3.1",
        "webpack": "^4.44.2",
        "webpack-cli": "^3.3.12"
    },
    "scripts": {
        "dev:server": "nodemon --exec babel-node src/init.js --delay 2 --ignore '.scss' --ignore 'static' ",
        "dev:assets": "cd src && WEBPACK_ENV=development webpack -w",
        "build:assets": "cd src && WEBPACK_ENV=production webpack",
        "build:server": "babel src --out-dir build --ignore 'src/assets','src/static','src/webpack.config.js'",
        "build": "npm run build:server && npm run build:assets && cp -R src/static build && cp -R src/views build",
        "prebuild": "rm -rf build",
        "start": "node build/init.js"
    },
    "devDependencies": {
        "babel-core": "^7.0.0-bridge.0",
        "eslint": "^7.10.0",
        "eslint-config-prettier": "^6.12.0",
        "eslint-plugin-prettier": "^3.1.4",
        "nodemon": "^2.0.4"
    }
}
